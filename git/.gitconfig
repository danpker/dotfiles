[user]
	name = Daniel Parker
	email = danpker@gmail.com
	signingkey = C7EFAD84A69CD8AA
[core]
	excludesfile = /Users/danielparker/.gitignore_global
	editor = vim
[color "diff"]
	old = red strike
	new = green italic
[alias]
        c = checkout
        ci = commit
        s = status
        br = branch
        type = cat-file -t
        dump = cat-file -p
        lol = log --oneline --graph --decorate
[merge]
	tool = vimdiff
	conflictstyle = diff3
[mergetool]
	prompt = false
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[commit]
	gpgsign = true
[init]
	templatedir = ~/.git-templates
